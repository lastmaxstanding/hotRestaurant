		<div class="row">
			<div class="col-lg-12">

				<!-- Current Tables in Use -->
				<div class="panel panel-default">
					<div class="panel-heading">
						<h4 class="panel-title">Current Reservations</h4>
					</div>
					<div class="panel-body" id="tableSection">

					 {{#each tables}}
						 <div class="well" id="tableWell-{{this.number}}">					 	
						 	<h2><span class="label label-primary">{{this.number}}</span> | {{this.customerID}}</h2>
						 </div>
					 {{/each}}

					</div>
				</div>
	
				<!-- Waiting List Panel -->
				<div class="panel panel-default">
					<div class="panel-heading">
						<h4 class="panel-title">Waiting List</h4>
					</div>
					<div class="panel-body" id="waitlistSection">
					{{#each waitListed}}
						<div class="well" id="waitlistWell-{{this.number}}">
							<h2><span class="label label-primary">{{this.number}}</span> | {{this.customerID}} </h2>
						</div>						
					{{/each}}

					</div>
				</div>
			</div>
		</div>
</html>